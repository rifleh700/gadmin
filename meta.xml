	<settings>
		<setting
			name="#pingkicker"
			value="0"
			group="Automatic scripts"
			friendlyname="Ping kicker"
			accept="0-1000"
			desc="Max ping allowed (0 - off)"
		/>
		<setting
			name="#fpskicker"
			value="0"
			group="Automatic scripts"
			friendlyname="FPS kicker"
			accept="0-100"
			desc="Lowest fps allowed: 25-75 (0 - off)"
		/>
		<setting
			name="#idlekicker"
			value="0"
			group="Automatic scripts"
			friendlyname="Idle Kicker"
			accept="0-100"
			desc="Maximum time a player can stay idle (minutes, 0 - off)"
		/>
		<setting
			name="#consolecommands"
			value="[true]"
			group="General"
			friendlyname="Enable console commands"
			accept="[true],[false]"
			desc="Parse console commands"
		/>
		<setting
			name="#reportscreenshots"
			value="[true]"
			group="Reports"
			friendlyname="Enable report screenshots"
			accept="[true],[false]"
			desc="Enable screenshot attaching to report"
		/>
		<setting
			name="#reportscreenshotsamount"
			value="4"
			group="Reports"
			friendlyname="Report screenshots amount"
			accept="0-20"
			desc="Maximum number of screenshots which can be attahed to report"
		/>
		<setting
			name="#reportscreenshotquality"
			value="80"
			group="Reports"
			friendlyname="Report screenshot quality"
			accept="1-100"
			desc="JPEG quality of screenshots which can be attahed to report"
		/>
		<setting
			name="#reportchathistory"
			value="[true]"
			group="Reports"
			friendlyname="Enable report client chat history"
			accept="[true],[false]"
			desc="Enable report client chat history"
		/>
		<setting
			name="#screenshotquality"
			value="80"
			group="Screenshots"
			friendlyname="Screenshot quality"
			accept="1-100"
			desc="JPEG quality of player's screenshot"
		/>
		<setting
			name="#screenshotclientcapture"
			value="[true]"
			group="Screenshots"
			friendlyname="Enable client screenshot capture"
			accept="[true],[false]"
			desc="Would be player screenshot requested by admin captured by client or server (true - client)"
		/>
		<setting
			name="#screenshotsamount"
			value="100"
			group="Client"
			friendlyname="Screenshots amount"
			accept="0-1000"
			desc="Maximum number of client's screenshots which would be stored"
		/>
		<setting
			name="#chatmessagesamount"
			value="100"
			group="Client"
			friendlyname="Max client chat messages"
			accept="0-500"
			desc="Maximum number of client's chat messages which would be stored"
		/>
		<setting
			name="#useip2c"
			value="[true]"
			friendlyname="Use IP to country"
			group="_Advanced"
			accept="[true],[false]"
			desc="Detect players IP country"
		/>
		<setting
			name="#fpsinterval"
			value="3000"
			group="_Advanced"
			friendlyname="Sync FPS interval"
			accept="200-60000"
			desc="Client FPS synchronization interval in ms"
		/>
		<setting
			name="#bigdatabandwith"
			value="50000"
			group="_Advanced"
			friendlyname="Sync big data bandwith"
			accept="200-60000"
			desc="Sync big data(i.e. report screenshots) bandwith in bytes/s"
		/>
	</settings>
</meta>
